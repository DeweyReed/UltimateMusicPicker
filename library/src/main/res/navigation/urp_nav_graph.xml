<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/urp_nav_graph"
    tools:ignore="InvalidNavigation,UnusedNavigation">
    <fragment
        android:id="@+id/urp_dest_system"
        android:name="xyz.aprildown.ultimateringtonepicker.ui.SystemRingtoneFragment"
        android:label="SystemRingtoneFragment"
        tools:layout="@layout/urp_recycler_view">
        <action
            android:id="@+id/urp_action_system_to_device"
            app:destination="@id/urp_dest_device" />
    </fragment>

    <fragment
        android:id="@+id/urp_dest_device"
        android:name="xyz.aprildown.ultimateringtonepicker.ui.DeviceRingtoneFragment"
        android:label="DeviceRingtoneFragment">
        <action
            android:id="@+id/urp_action_device_to_list"
            app:destination="@id/urp_dest_ringtone_list" />
    </fragment>
    <fragment
        android:id="@+id/urp_dest_ringtone_list"
        android:name="xyz.aprildown.ultimateringtonepicker.ui.RingtoneFragment" />
</navigation>